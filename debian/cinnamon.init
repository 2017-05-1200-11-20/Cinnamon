#! /bin/sh
### BEGIN INIT INFO
# Provides:          cinnamon
# Default-Start:     2 3 4 5
# Default-Stop:      0 1 6
# Short-Description: Cinnamon cache preloader
### END INIT INFO

set -e

case "$1" in
  start)
        su -c cinnamon-init-cache
  ;;
  *)
        echo "Usage: /etc/init.d/cinnamon {start}"
        exit 1
  ;;
esac

exit 0
