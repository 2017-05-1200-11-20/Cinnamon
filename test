#!/bin/bash

rm -rf /usr/share/gnome-shell/js-backup
mv /usr/share/gnome-shell/js /usr/share/gnome-shell/js-backup
cp -R js /usr/share/gnome-shell/

rm -rf /usr/share/gnome-shell/theme-backup
mv /usr/share/gnome-shell/theme /usr/share/gnome-shell/theme-backup
cp -R data/theme /usr/share/gnome-shell/
